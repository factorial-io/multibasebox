<div class="for-overview {{content.overview_color}}">
  <nav>
  {{content.houses_nav}}
  </nav>

  <div class="overview-row">
    <section class="area-stage">
      <div class="area-stage__images">
        {{content.field_image}}
      </div>
    </section>
    <section class="overview-info">
      <h1>{{label}}</h1>

      {{content.body}}

      <h3 class="where-what">{{'Where is What in @house'|t({'@house': label|render})}}</h3>
      {{content.circles}}
    </section>
  </div>

  <h2>{{'Rental'|t}}</h2>

  {{content.field_floors}}
  <div class="house-svg" data-svg-url="{{file_url(content.field_house_svg[0]['#file'].getFileUri())}}">
  
  </div>

  {% include '@circle/partials/area_contact.html.twig' %}
</div>