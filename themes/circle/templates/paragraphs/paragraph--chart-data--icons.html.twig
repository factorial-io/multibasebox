<div class="chart-data-icon">
  <div class="chart-data-icon__icon {{content.field_key[0]['#context'].value}}">
    <img src="{{file_url(content.field_svg_icon[0]['#file'].getFileUri())}}">
  </div>
  <p class="chart-data-icon__name">{{content.field_name}}</p>
  <p class="chart-data-icon__value" data-count-up="{{content.field_value.0['#markup']}}">{{content.field_value}}</p>
</div>
